		</xs:complexType>
	</xs:element>
	<xs:element name="Baja">
		<xs:annotation>
			<xs:documentation>Datos de cada baja del vehículo consultado</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="FechaInicio" minOccurs="0"/>
				<xs:element ref="FechaFin" minOccurs="0"/>

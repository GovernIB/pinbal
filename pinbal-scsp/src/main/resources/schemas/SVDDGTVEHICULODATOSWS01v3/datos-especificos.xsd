		<xs:simpleType>
			<xs:restriction base="xs:float"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DatosTramites">
		<xs:annotation>
			<xs:documentation>Datos de los trámites del vehículo consultado</xs:documentation>

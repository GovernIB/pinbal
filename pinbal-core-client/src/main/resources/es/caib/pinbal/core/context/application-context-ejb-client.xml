			id="scspService"
			jndi-name="pinbal/ScspServiceBean/local"
			business-interface="es.caib.pinbal.core.service.ScspService"/>
	<jee:local-slsb
			id="aclService"
			jndi-name="pinbal/AclServiceBean/local"
			business-interface="org.springframework.security.acls.model.AclService"/>
	<jee:local-slsb
			id="organGestorService"
			jndi-name="pinbal/OrganGestorServiceBean/local"
			business-interface="es.caib.pinbal.core.service.OrganGestorService"/>
	<jee:local-slsb
			id="recobrimentService"
			jndi-name="pinbal/RecobrimentServiceBean/local"
			business-interface="es.caib.pinbal.core.service.RecobrimentService"/>
	<jee:local-slsb
			id="aplicacioService"
			jndi-name="pinbal/AplicacioServiceBean/local"
			business-interface="es.caib.pinbal.core.service.AplicacioService"/>
</beans>

     <xs:element minOccurs="0" name="apellido2" type="xs:string"/>
     <xs:element minOccurs="0" name="documentacion" type="xs:string"/>
     <xs:element minOccurs="0" name="nombre" type="xs:string"/>
     <xs:element minOccurs="0" name="nombreCompleto" type="xs:string"/>
     <xs:element minOccurs="0" name="tipoDocumentacion" type="tns:tipoDocumentacion"/>
    </xs:sequence>
   </xs:complexType>
   <xs:complexType name="transmision">
    <xs:sequence>
     <xs:element minOccurs="0" name="codigoCertificado" type="xs:string"/>
     <xs:element minOccurs="0" name="fechaGeneracion" type="xs:string"/>
     <xs:element minOccurs="0" name="idSolicitud" type="xs:string"/>
     <xs:element minOccurs="0" name="idTransmision" type="xs:string"/>

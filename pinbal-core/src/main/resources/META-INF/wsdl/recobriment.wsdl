    </xs:sequence>
   </xs:complexType>
   <xs:complexType name="transmisionDatos">
    <xs:sequence>
     <xs:element minOccurs="0" name="datosEspecificos" type="xs:anyType"/>
     <xs:element minOccurs="0" name="datosGenericos" type="tns:datosGenericos"/>
     <xs:element minOccurs="0" name="id" type="xs:string"/>
    </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="consentimiento">
    <xs:restriction base="xs:string">
     <xs:enumeration value="Si"/>
     <xs:enumeration value="Ley"/>
    </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="tipoDocumentacion">
    <xs:restriction base="xs:string">
     <xs:enumeration value="CIF"/>
     <xs:enumeration value="NIF"/>
     <xs:enumeration value="DNI"/>
     <xs:enumeration value="Pasaporte"/>
     <xs:enumeration value="NIE"/>
    </xs:restriction>
   </xs:simpleType>

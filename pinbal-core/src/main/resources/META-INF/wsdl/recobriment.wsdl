     <xs:element minOccurs="0" name="id" type="xs:string"/>
    </xs:sequence>
   </xs:complexType>
   <xs:complexType name="datosGenericos">
    <xs:sequence>
     <xs:element minOccurs="0" name="emisor" type="tns:emisor"/>
     <xs:element minOccurs="0" name="solicitante" type="tns:solicitante"/>

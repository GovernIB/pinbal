<definitions name="RecobrimentService" targetNamespace="http://www.caib.es/pinbal/ws/recobriment" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.caib.es/pinbal/ws/recobriment" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

<decorators defaultdir="/WEB-INF/jsp/decorators">
	<excludes>
		<pattern>/js*</pattern>
		<pattern>/css*</pattern>
		<pattern>/img*</pattern>
		<pattern>/ico*</pattern>
		<pattern>/ws*</pattern>
		<pattern>/api/apidoc</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="modal" page="modal.jsp">
		<pattern>/modal*</pattern>
	</decorator>
</decorators>
